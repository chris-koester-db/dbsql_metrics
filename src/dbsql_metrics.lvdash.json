{
  "datasets": [
    {
      "name": "temp_dataset_name",
      "displayName": "metv_query_history_daily",
      "queryLines": [
        "select\n",
        "  date,\n",
        "  measure(query_count) as query_count,\n",
        "  measure(query_duration_sec_avg) as query_duration_sec_avg,\n",
        "  measure(distinct_users) as distinct_users\n",
        "from\n",
        "  users.chris_koester.metv_query_history\n",
        "where date between :param_date.min and :param_date.max\n",
        "group by all"
      ],
      "parameters": [
        {
          "displayName": "param_date",
          "keyword": "param_date",
          "dataType": "DATE",
          "complexType": "RANGE",
          "defaultSelection": {
            "range": {
              "dataType": "DATE",
              "min": {
                "value": "now-90d/d"
              },
              "max": {
                "value": "now/d"
              }
            }
          }
        }
      ]
    },
    {
      "name": "5142b18c",
      "displayName": "param_months",
      "queryLines": [
        "select *\n",
        "from values ('Jan'), ('Feb'), ('Mar'), ('Apr'), ('May'), ('Jun'), ('Jul'), ('Aug'), ('Sep'), ('Oct'), ('Nov'), ('Dec') as data(month)"
      ]
    },
    {
      "name": "40967098",
      "displayName": "metv_query_history_monthly",
      "queryLines": [
        "select\n",
        "  year,\n",
        "  month,\n",
        "  measure(query_count) as query_count,\n",
        "  measure(query_duration_sec_avg) as query_duration_sec_avg,\n",
        "  measure(query_duration_sec_sum) as query_duration_sec_sum\n",
        "  -- measure(distinct_users_t7d) as distinct_users_t7d\n",
        "from\n",
        "  main.chris_koester.metv_query_history\n",
        "where year = :year\n",
        "and array_contains(:month, month)\n",
        "group by all"
      ],
      "parameters": [
        {
          "displayName": "month",
          "keyword": "month",
          "dataType": "STRING",
          "complexType": "MULTI",
          "defaultSelection": {
            "values": {
              "dataType": "STRING",
              "values": [
                {
                  "value": "Jan"
                },
                {
                  "value": "Feb"
