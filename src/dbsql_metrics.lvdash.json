{
  "datasets": [
    {
      "name": "temp_dataset_name",
      "displayName": "query_hist_daily",
      "queryLines": [
        "select\n",
        "  date,\n",
        "  workspace_name,\n",
        "  client_application,\n",
        "  compute_type,\n",
        "  warehouse_name,\n",
        "  warehouse_type,\n",
        "  warehouse_size,\n",
        "  measure(query_count) as query_count,\n",
        "  measure(distinct_users) as distinct_users,\n",
        "  measure(query_duration_hrs_sum) as query_duration_hrs_sum\n",
        "from\n",
        "  identifier(:catalog || '.' || :schema || '.metv_dbsql')\n",
        "where compute_type = 'Warehouse'\n",
        "and date between :param_date.min and :param_date.max\n",
        "group by all"
      ],
      "parameters": [
        {
          "displayName": "catalog",
          "keyword": "catalog",
          "dataType": "STRING",
          "defaultSelection": {
            "values": {
              "dataType": "STRING",
              "values": [
                {
                  "value": "users"
                }
              ]
            }
          }
        },
        {
          "displayName": "schema",
          "keyword": "schema",
          "dataType": "STRING",
          "defaultSelection": {
            "values": {
              "dataType": "STRING",
              "values": [
                {
                  "value": "chris_koester"
                }
              ]
            }
          }
        },
        {
          "displayName": "param_date",
          "keyword": "param_date",
          "dataType": "DATE",
          "complexType": "RANGE",
          "defaultSelection": {
            "range": {
              "dataType": "DATE",
              "min": {
                "value": "now-28d/d"
              },
              "max": {
                "value": "now/d"
              }
            }
          }
        }
      ]
    }
  ],
