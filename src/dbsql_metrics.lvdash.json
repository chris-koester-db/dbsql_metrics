          "dataType": "DECIMAL",
          "defaultSelection": {
            "values": {
              "dataType": "DECIMAL",
              "values": [
                {
                  "value": "2025"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "c985c9d7",
      "displayName": "param_current_date",
      "queryLines": [
        "select current_date() as date"
      ]
